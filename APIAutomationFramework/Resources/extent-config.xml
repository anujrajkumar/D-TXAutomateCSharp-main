		<!-- report name - displayed at top-nav -->
		<reportName>ebTax</reportName>

		<!-- custom javascript -->
		<scripts>
			<![CDATA[

            ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
			<![CDATA[

            ]]>
		</styles>
	</configuration>
</extentreports>
